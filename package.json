{
  "name": "@femessage/v-editor",
  "version": "1.2.0",
  "description": "upload img and write rich text easily",
  "author": "https://github.com/FEMessage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FEMessage/v-editor.git"
  },
  "keywords": ["vue", "sfc", "component"],
  "files": ["src", "dist", "yarn.lock"],
  "main": "dist/v-editor.umd.js",
  "module": "dist/v-editor.esm.js",
  "unpkg": "dist/v-editor.min.js",
  "browser": {
    "./sfc": "src/v-editor.vue"
  },
  "scripts": {
    "dev": "vue-styleguidist server",
    "test": "jest",
    "doc": "vue-styleguidist build",
    "build":
      "npm run build:unpkg & npm run build:es & npm run build:umd & npm run doc",
    "build:umd":
      "rollup --config build/rollup.config.js --format umd --file dist/v-editor.umd.js",
    "build:es":
      "rollup --config build/rollup.config.js --format es --file dist/v-editor.esm.js",
    "build:unpkg":
      "rollup --config build/rollup.config.js --format iife --file dist/v-editor.min.js",
    "precommit": "pretty-quick --staged",
    "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
    "release": "gren release --override",
    "lint": "eslint \"src/**/*.@(js|vue)\" --fix"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-adapter-ckfinder": "^16.0.0",
    "@ckeditor/ckeditor5-autoformat": "^16.0.0",
    "@ckeditor/ckeditor5-basic-styles": "^16.0.0",
    "@ckeditor/ckeditor5-block-quote": "^16.0.0",
    "@ckeditor/ckeditor5-ckfinder": "^16.0.0",
    "@ckeditor/ckeditor5-core": "^16.0.0",
    "@ckeditor/ckeditor5-dev-utils": "^12.0.0",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^8.0.0",
    "@ckeditor/ckeditor5-easy-image": "^16.0.0",
    "@ckeditor/ckeditor5-editor-classic": "^16.0.0",
    "@ckeditor/ckeditor5-essentials": "^16.0.0",
    "@ckeditor/ckeditor5-font": "^16.0.0",
    "@ckeditor/ckeditor5-heading": "^16.0.0",
    "@ckeditor/ckeditor5-horizontal-line": "^16.0.0",
    "@ckeditor/ckeditor5-image": "^16.0.0",
    "@ckeditor/ckeditor5-indent": "^16.0.0",
    "@ckeditor/ckeditor5-link": "^16.0.0",
    "@ckeditor/ckeditor5-list": "^16.0.0",
    "@ckeditor/ckeditor5-media-embed": "^16.0.0",
    "@ckeditor/ckeditor5-paragraph": "^16.0.0",
    "@ckeditor/ckeditor5-paste-from-office": "^16.0.0",
    "@ckeditor/ckeditor5-table": "^16.0.0",
    "@ckeditor/ckeditor5-theme-lark": "^16.0.0",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@femessage/github-release-notes": "^0.19.0",
    "@femessage/upload-to-ali": "^2.0.1",
    "babel-loader": "^8.0.5",
    "babel-eslint": "^10.0.3",
    "dotenv": "^7.0.0",
    "element-ui": "^2.7.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "minimist": "^1.2.0",
    "postcss-loader": "^3.0.0",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "raw-loader": "^4.0.0",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "standard-version": "^6.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.16",
    "vue-loader": "^15.7.0",
    "vue-styleguidist": "3.11.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.29.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "vue-sfc-cli": "1.11.2",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "gren": "@femessage/grenrc"
}
